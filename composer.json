{
    "name": "aoe/truncate-job",
    "description": "Scheduler Job to truncate tables",
    "type": "typo3-cms-extension",
    "homepage": "https://forge.typo3.org/projects/extension-truncate_job",
    "license": "GPL-3.0",
    "support": {
        "email": "dev@aoe.com",
        "issues": "https://forge.typo3.org/projects/extension-truncate_job/issues",
        "source": "https://forge.typo3.org/projects/extension-truncate_job"
    },
    "authors": [
        {
            "name": "AOE GmbH",
            "email": "dev@aoe.com",
            "homepage": "http://www.aoe.com"
        }
    ],
    "require": {
        "php": ">=5.1.0",
        "typo3/cms-core": "*"
    },
    "require-dev": {
        "typo3/cms": "6.2.*",
        "phpunit/phpunit": "4.8.*",
        "phpunit/phpcov": "2.0.*",
        "squizlabs/php_codesniffer": "2.3.*",
        "sebastian/phpcpd": "2.0.*",
        "phpmd/phpmd": "2.2.*"
    },
    "autoload": {
        "classmap": [
            "Classes"
        ]
    },
    "replace": {
        "truncate_job": "*"
    },
    "keywords": [
        "TYPO3",
        "extbase",
        "truncate-job"
    ]
